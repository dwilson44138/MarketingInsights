{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "DWilson-DataFactory"
		},
		"asynapseMarketingInsights_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'asynapseMarketingInsights'"
		},
		"lsMarketingInsights_functionKey": {
			"type": "secureString",
			"metadata": "Secure string for 'functionKey' of 'lsMarketingInsights'"
		},
		"AzureBlobStorage1_properties_typeProperties_serviceEndpoint": {
			"type": "string",
			"defaultValue": "https://speedeondatalake.blob.core.windows.net"
		},
		"EnduranceDirect_Custom_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://speedeondatalake.dfs.core.windows.net"
		},
		"lsMarketingInsights_properties_typeProperties_functionAppUrl": {
			"type": "string",
			"defaultValue": "https://marketinginsights.azurewebsites.net"
		},
		"AzureDataLakeStorage1_properties_privateLinkResourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/ebcf966c-31a5-468b-ba6f-3ab0777856a8/resourceGroups/northcentralproductionrg/providers/Microsoft.Storage/storageAccounts/speedeondatalake"
		},
		"AzureDataLakeStorage1_properties_groupId": {
			"type": "string",
			"defaultValue": "dfs"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "DWilson-DataFactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"asynapseMarketingInsights_connectionString": {
						"value": "[parameters('asynapseMarketingInsights_connectionString')]"
					},
					"lsMarketingInsights_functionKey": {
						"value": "[parameters('lsMarketingInsights_functionKey')]"
					},
					"AzureBlobStorage1_properties_typeProperties_serviceEndpoint": {
						"value": "[parameters('AzureBlobStorage1_properties_typeProperties_serviceEndpoint')]"
					},
					"lsMarketingInsights_properties_typeProperties_functionAppUrl": {
						"value": "[parameters('lsMarketingInsights_properties_typeProperties_functionAppUrl')]"
					},
					"AzureDataLakeStorage1_properties_privateLinkResourceId": {
						"value": "[parameters('AzureDataLakeStorage1_properties_privateLinkResourceId')]"
					},
					"AzureDataLakeStorage1_properties_groupId": {
						"value": "[parameters('AzureDataLakeStorage1_properties_groupId')]"
					},
					"EnduranceDirect_Custom_properties_typeProperties_url": {
						"value": "[parameters('EnduranceDirect_Custom_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}